/**界限 2018/09/01 界限*/

.tem-navigationBar{
  position:fixed;
  display:inline-flex;
}
.tem-navigationBar-mask{
  position:absolute;
  top:0;
  z-index: -1;
  width:100%;
  height:100%;
  opacity:0.8;
  background-color:#fff;
}
.tem-navigation-icon{
  width:32px;
  height:32px;
  margin:7px;
  visibility:visible;
  border-radius:50rpx;
  transition: transform 0.3s,width 0.3s,height 0.3s;
  vertical-align:top;
}
.unread-dot{
  position:absolute;
  background-color:#f00;
  top:7px;
  right:7px;
  width:2mm;
  height:2mm;
  border-radius:50%;
  /**
  transition:all 1s;
  -moz-transition:all 1s;
  -webkit-transitino:all 1s;
  */
}
.tem-navigation-icon:hover{
  border-radius:50%;
  opacity:0.8;
  transform: rotate(90deg);
  filter:brightness(80%);
}
.tem-navigation-title{
  display:inline-block;
  vertical-align:top;
  height:32px;
  line-height:32px;
  padding:7px 10px 7px 0;
  visibility:visible;
  font-size:20px;
}

/*template: search input box*/
.searchBoxContent{
  position:relative;
  display:flex;
  width:calc((100vw - 84rpx - 94pt) *1);
  height:calc(32px * 1);
  margin:7px 10px 7px 0;
  line-height:32px;
  font-size:14px;
  border-radius:5px;
  background-color:#fff;
}
.searchInput{
  position:relative;
  padding-left:16px;
  width:calc(100% - 48px);
  height:32px;
  color:#888;
}
.searchClear{
  position:relative;
  right:4px;
  width:20px;
  height:20px;
  padding:6px;
}
.hoverClear{
  opacity:0.3;
}
.searchLoading{
  position:relative;
  width:60rpx;
  height:60rpx;
  left:44%;
  top:8rpx;
}

