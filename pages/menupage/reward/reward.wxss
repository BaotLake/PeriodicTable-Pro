/* pages/menupage/reward/reward.wxss */
@import '../../data/template.wxss';

.rewardPage{
  padding:60rpx 0;
  height:100vh;
  background-color:#fff;
  text-align:center;
}

.rewardText{
  display:block;
  padding:120rpx 40rpx 60rpx 40rpx;
  color:#888;
  font-size:32rpx;
}
.rewardNote{
  display:block;
  margin:40rpx 0 0 0;
  color:#474b4c;
  font-size:36rpx;
}
.WXreward{
  display:block;
  position:relative;
  width:700rpx;
  height:700rpx;
  margin:auto;
}
