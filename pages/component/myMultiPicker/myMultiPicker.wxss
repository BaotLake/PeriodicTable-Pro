.myMultiPicker{
  position:fixed;
  z-index:999;
}
.fullMasking{
  position:fixed;
  width:750rpx;
  height:100vh;
  left:0;
  top:0;
  background-color: #000;
  opacity:0.3;
}
.pickerBlock{
  position:fixed;
  width:700rpx;
  height:40vh;
  bottom:0;
  left:0;
  background-color:#fff;
  text-align:center;
  padding:25rpx;
}
.myMultiPickerCancel{
  width:120rpx;
  height:80rpx;
  line-height:80rpx;
  position:absolute;
  display:inline-block;
  top:0;
  left:0;
  color:#999;
}
.myMultiPickerOk{
  width:120rpx;
  height:80rpx;
  line-height:80rpx;
  position:absolute;
  display:inline-block;
  top:0;
  right:0;
  color:#0f0;
}
.MultiPickerRankTitle{
  position:relative;
  margin-top:80rpx;
  left:0;
  display:inline-block;
  margin-left:auto;
  margin-right:auto;
}
.myMultiPickerView{
  position:relative;
  width:100%;
  height:300rpx;
}
.myMultiPickerViewColumn{

}
.PickerColumnBox{
  height:60rpx;
  line-height:80rpx;
}
.pickerBox-class{
  display:block;
  
}