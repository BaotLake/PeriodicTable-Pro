.navigationBar-Back-Content{
  position:fixed;
  top: 58rpx;
  left:20rpx;
  width: 66rpx;
  height: 66rpx;
}
.navigationBar-Back{
  position:absolute;
  width:100%;
  height:100%;
  border: 1rpx solid #aaa;
  border-radius:20%;
  color:#fff;
}
.navigationBar-Back-image{
  position:relative;
  top:10%;
  left:10%;
  width:80%;
  height:80%;
}

.foot-wrapper{
  position:absolute;
  bottom:0;
  left:0;
  width:100%;
  display:flex;
  justify-content:center;
}
.foot{
  display:flex;
  padding:10px 0;
  align-items:center;
}
.footImage{
  position:relative;
  width:1.2rem;
  height:1.2rem;
  border-radius:50%;
  border:1px solid rgba(var(--fore-color-1), 0.3);
}
.footText{
  position:relative;
  line-height:1.5rem;
  font-size:0.8rem;
  margin: 0 0 0 10rpx;
  color:rgb(var(--fore-color-1));
}


.QABlock{
  padding:1.2rem;
  background-color:rgb(var(--back-color-2));
  border-radius:1.2rem;
  text-align:center;
  position:relative;
  border:1px solid #ddd;
  width:15rem;
  margin-left:auto;
  margin-right:auto;
  margin-top:1rem;
  margin-bottom:1rem;
  color:rgb(var(--fore-color-1));
}
.QAText{
  margin:auto;
  padding:0.5rem 0 1rem 0;
  font-size:1rem;
}
.buttonBlock{
  height:2.5rem;
  display:flex;
  font-size:1rem;
}
.QA-button{
  margin:auto;
}

.ChemicalFormula{
  position:relative;
  font-size:1em;
}
.CFSub{
  position:relative;
  top:0.5em;
  font-size:0.6em;
}
.CFSup{
  position:relative;
  top:-0.5em;
  font-size:0.6em;
}
.CFNormal{
  position:relative;
  font-size:1em;
}

.shareBlock{
  margin:20px 0 0 0;
  padding: 0 30px;
  color:#fff;
}


/**界限 2018/09/01 界限*/

.tem-navigationBar{
  position:fixed;
  display:inline-flex;
}
.tem-navigationBar-mask{
  position:absolute;
  top:0;
  z-index: -1;
  width:100%;
  height:100%;
  opacity:0.8;
  background-color:#fff;
}
.tem-navigation-icon{
  width:32px;
  height:32px;
  padding:6px;
  visibility:visible;
  border-radius:50rpx;
  transition: transform 0.3s,width 0.3s,height 0.3s;
  vertical-align:top;
  box-sizing:border-box;
}
.unread-dot{
  position:absolute;
  background-color:#f00;
  top:7px;
  right:7px;
  width:2mm;
  height:2mm;
  border-radius:50%;
  /**
  transition:all 1s;
  -moz-transition:all 1s;
  -webkit-transitino:all 1s;
  */
}

.tem-navigation-title{
  display:inline-block;
  vertical-align:top;
  height:32px;
  line-height:32px;
  padding:7px 10px 7px 0;
  visibility:visible;
  font-size:20px;
}

.hoverClear{
  opacity:0.3;
}
.searchLoading{
  position:relative;
  width:60rpx;
  height:60rpx;
  left:44%;
  top:8rpx;
}

.tem-rect-tool-button{
  display:inline-block;
  width:60px;
  height:90px;
  border-radius:5px;
  /**border: 1px solid #000;*/
  margin:10px 10px;
}
.tem-rect-tool-image{
  display:block;
  width:60px;
  height:60px;
  padding:15px;
  box-sizing:border-box;
  border-radius:50%;
  background-color:rgb(var(--back-color-3));
}
.tem-rect-tool-svg{
  width:100%;
  height:100%;
}
.tem-rect-tool-text{
  font-size:15px;
  height:30px;
  line-height:30px;
  text-align:center;
  font-size:0.8em;
  color:rgb(var(--fore-color-1));
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
