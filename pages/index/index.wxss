.css-icon-search{
  position:relative;
}

.css-icon-search::before{
  position:absolute;
  top:0;
  left:0;
  width:70%;
  height:70%;
  border-radius:50%;
  border:2px solid rgb(var(--fore-color-1));
  content:' ';
  display:block;
  transition:all 0.3s ease-in-out;
  transform:rotate(45deg);
  box-sizing:border-box;
}
.css-icon-search::after{
  position:absolute;
  bottom:0;
  right:0;
  width:56.924%;
  height:0px;
  box-shadow: 0 0 0 1px rgb(var(--fore-color-1));
  content:' ';
  display:block;
  transform:rotate(45deg);
  transform-origin:right bottom;
  transition:all 0.3s ease-in-out;
}

.css-icon-search-go::before{
  border-left-color:transparent !important;
  border-bottom-color:transparent !important;
  width:70% !important;
  height:70% !important;
  transform:rotate(45deg) !important;
  border-radius:0 !important;
  top:15% !important;
  left:30% !important;
}
.css-icon-search-go::after{
  transform:rotate(0) !important;
  bottom:50% !important;
  width:80% !important;
}


